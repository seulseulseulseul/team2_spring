	<select id = "getTrainer" resultType="map" parameterMap="java.lang.String">
	
	<!-- 이게 호출 인자에 따라서 map형태 아니면 list 형태로 반환됨 ok
	저기 내부에 이제 dto가 잇다고 생각하면 이해하기쉬울듯 
	 --> 
	
	SELECT * 	
	 T_NUM
	,T_ID
	,T_PASS
	,T_NAME
	,T_NIC
	,T_ADDRESS
	,T_INTRO
	,T_PHONE
	,T_EMAIL
	,T_CASH
	,T_EXP
	,T_SNS
	,T_PROGRAM
	,PROFILE_PHOTO
	,T_DETAILADDRESS
	,VIDEO
	,T_POSTECODE
	  
	FROM TRAINER
	
   WHERE 1=1 	
<!-- 	<isNotEmpty prepend="AND" property="TID"> -->
	 AND T_ID = #{TID}
<!-- 	</isNotEmpty> -->
	
	 
	
	
	
	</select>
	
