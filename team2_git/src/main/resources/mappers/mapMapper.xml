<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.itwill.mapper.MapMapper">

	<select id = "getAddress" resultType="com.itwill.domain.TrainerDTO">
		select * from trainer where t_extraAddress like concat('%',#{t_extraAddress},'%')
	</select>
  
</mapper>